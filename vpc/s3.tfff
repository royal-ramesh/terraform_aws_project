resource "aws_s3_bucket" "tfstate" {
    bucket = "ramesh-tf-practice-mumbai-region"
    versioning {
      enabled = true
    }
  
}

resource "aws_dynamodb_table" "terraform_locks" {
  name         = "terraform-locks"
  billing_mode = "PAY_PER_REQUEST"
  hash_key     = "LockID"

  attribute {
    name = "LockID"
    type = "S"
  }

  tags = {
    Name = "Terraform Lock Table"
    Environment = "dev"
  }
}